{
  "name": "timhortons-search-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --module-bind 'js=babel-loader' --module-bind 'jsx=babel-loader' --resolve-extensions .js,.jsx --mode production --entry ./src --output public/bundle.js",
    "watch": "webpack --module-bind 'js=babel-loader' --module-bind 'jsx=babel-loader' --resolve-extensions .js,.jsx --mode production --entry ./src --output public/bundle.js --watch",
    "seed": "knex seed:run --knexfile ./server/db/knexfile.js",
    "migrate": "knex migrate:latest --knexfile ./server/db/knexfile.js",
    "rollback": "knex migrate:rollback --knexfile ./server/db/knexfile.js",
    "server": "node ./server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/okasato/timhortons-search-app.git"
  },
